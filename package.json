{
  "name": "stackathon",
  "main": "node_modules/expo/AppEntry.js",
  "engines": {
    "node": ">= 7.0.0"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  },
  "scripts": {
    "start": "expo start",
    "start-server": "nodemon index.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "seed": "node script/seed.js",
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "start-node-server": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' npm run start-server",
    "test:server": "mocha ./server/db/tests.spec.js",
    "test:client": "mocha ./app/components/tests.spec.js --require babel-core/register --require ./testingSetup.js",
    "creature-feature-quest-test": "npm run test:server & npm run test:client"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "connect-session-sequelize": "^6.0.0",
    "expo": "^32.0.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "react": "16.5.0",
    "react-dom": "^16.7.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-navigation": "^3.0.9",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^4.42.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-expo": "^5.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "jest-enzyme": "^7.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "react-test-renderer": "^16.4.0",
    "sinon": "^5.1.0",
    "supertest": "^3.4.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3"
  },
  "eslintConfig": {
    "rules": {
      "quotes": "off"
    },
    "parser": "babel-eslint"
  },
  "private": true
}
